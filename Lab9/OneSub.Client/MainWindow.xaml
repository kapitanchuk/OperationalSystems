<Window x:Class="OneSub.Client.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:OneSub.Client"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto"/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <StackPanel>
            <CheckBox x:Name="CurrecncyCheckBox" Checked="CurrecncyCheckBox_Checked" Unchecked="CurrecncyCheckBox_Unchecked">Currencies</CheckBox>
            <CheckBox x:Name="WeatherCheckBox" Checked="WeatherCheckBox_Checked" Unchecked="WeatherCheckBox_Unchecked">Weather</CheckBox>
            <CheckBox x:Name="StocksCheckBox" Checked="StocksCheckBox_Checked" Unchecked="StocksCheckBox_Unchecked">Stocks</CheckBox>
        </StackPanel>
            <TextBox x:Name="MsgBox" Grid.Row="1"></TextBox>
    </Grid>
</Window>
